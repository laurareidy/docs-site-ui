<tr class="tr">
  <td class="td">
    <img class="img" src="{{@root.uiRootPath}}/img/icons/connectors/icon-{{delete-suffix name '-connector'}}.svg" width="30" alt="{{delete-suffix title ' Connector'}} logo">
  </td>
  <td class="td">{{delete-suffix title ' Connector'}}</td>
  <td class="td">
    {{#if (eq latest.version '0')}}
    <a class="link mule3" href="{{relativize url}}">Mule 3</a>
    {{else if (eq oldest.version '0')}}
    <a class="link mule4" href="{{relativize url}}">Mule 4</a>,
    <a class="link mule3" href="{{relativize oldest.url}}">Mule 3</a>
    {{else}}
    <a class="link mule4" href="{{relativize url}}">Mule 4</a>
    {{/if}}
  </td>
  <td class="td">{{attrs.page-connector-type}}</td>
  <td class="td">
    <button class="button js-connector-menu-trigger">
      <svg class="svg" width="16" height="16" viewBox="0 0 16 16" xmlns="http://www.w3.org/2000/svg">
        <path d="M13.5 9.5C14.3284 9.5 15 8.82843 15 8C15 7.17157 14.3284 6.5 13.5 6.5C12.6716 6.5 12 7.17157 12 8C12 8.82843 12.6716 9.5 13.5 9.5Z" />
        <path d="M8 9.5C8.82843 9.5 9.5 8.82843 9.5 8C9.5 7.17157 8.82843 6.5 8 6.5C7.17157 6.5 6.5 7.17157 6.5 8C6.5 8.82843 7.17157 9.5 8 9.5Z" />
        <path d="M2.5 9.5C3.32843 9.5 4 8.82843 4 8C4 7.17157 3.32843 6.5 2.5 6.5C1.67157 6.5 1 7.17157 1 8C1 8.82843 1.67157 9.5 2.5 9.5Z" />
      </svg>
    </button>
{{> connectors-menu}}
  </td>
</tr>
